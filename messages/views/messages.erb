<html>
  <head>
    <meta charset="utf-8" />
    <title></title>
    <link href="style.css" rel="stylesheet" />
  </head>
  <body>

    <div class="header">My Messages</div>

    <div id="message-stream">
      <% @messages.each do |m| %>;
      <div class="message">
      <a href="" class="close"></a>
      <div class="from">
        m.from %>
      </div>
      <div class="content">
        <%= m.content %>
      </div>
      <div class="time">
        <%= m.created_at %>
      </div>
    </div>
    <% end %>
    </div>
  </body>
</html>
